    "prosemirror-model": "^1.19.4",
    "prosemirror-schema-basic": "^1.2.2",
    "prosemirror-state": "^1.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^2.1.5",
    "react-i18next": "^14.0.1",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "twitter-api-v2": "^1.16.2",
